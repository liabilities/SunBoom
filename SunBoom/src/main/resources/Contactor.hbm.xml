<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="pojo.Contactor" table="contactor" schema="rsp" dynamic-insert="true">
        <id name="rowID">
            <column name="rowID" sql-type="int" />
            <generator class="increment"/>
        </id>
        <property name="idA">
            <column name="idA" sql-type="int"/>
        </property>
        <property name="idB">
            <column name="idB" sql-type="int"/>
        </property>

    </class>
</hibernate-mapping>