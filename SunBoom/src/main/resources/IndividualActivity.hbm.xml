<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="pojo.IndividualActivity" table="individualactivity" schema="rsp" dynamic-insert="true">
        <id name="individual_activityID">
            <column name="individual_activityID" sql-type="int" />
            <generator class="increment"/>
        </id>
        <property name="activityID">
            <column name="activityID" sql-type="int"/>
        </property>
        <property name="startTime">
            <column name="startTime" sql-type="date"/>
        </property>
        <property name="responsorNum">
            <column name="responsorNum" sql-type="int"/>
        </property>
        <property name="neededNum">
            <column name="neededNum" sql-type="int"/>
        </property>
        <property name="state">
            <column name="state" sql-type="int(1)"/>
        </property>


    </class>
</hibernate-mapping>