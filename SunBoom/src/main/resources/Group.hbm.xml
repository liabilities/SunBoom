<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="pojo.Group" table="group" schema="rsp" dynamic-insert="true">
        <id name="groupID">
            <column name="groupID" sql-type="int(11)" />
        </id>
        <property name="schoolID">
            <column name="schoolID" sql-type="int(11)"/>
        </property>
        <property name="tag">
            <column name="tag" sql-type="varchar(45)"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(45)" />
        </property>
        <property name="signature">
            <column name="signature" sql-type="varchar(45)"/>
        </property>
        <property name="introduction">
            <column name="introduction" sql-type="text"/>
        </property>
        <property name="avatar">
            <column name="avatar" sql-type="varchar(255)" />
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(45)"/>
        </property>
        <property name="memberNum">
            <column name="memberNum" sql-type="int(11)"/>
        </property>
        <property name="userName">
            <column name="userName" sql-type="varchar(45)"/>
        </property>
        <property name="tel">
            <column name="tel" sql-type="varchar(11)"/>
        </property>
        <property name="mail">
            <column name="mail" sql-type="varchar(45)"/>
        </property>
        <property name="detailPath">
            <column name="detailPath" sql-type="varchar(45)"/>
        </property>
        <property name="registerTime">
            <column name="registerTime" sql-type="DATE"/>
        </property>
        <property name="identificationTime">
            <column name="identificationTime" sql-type="DATE"/>
        </property>
        <property name="deadline">
            <column name="deadline" sql-type="DATE"/>
        </property>
        <property name="fellowNum_group">
            <column name="fellowNum_group" sql-type="int(11)"/>
        </property>
        <property name="fellowNum_person">
            <column name="fellowNum_person" sql-type="int(11)"/>
        </property>
        <property name="certifiedMaterial">
            <column name="certifiedMaterial" sql-type="varchar(255)"/>
        </property>

    </class>
</hibernate-mapping>