<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="pojo.Code" table="code" schema="rsp" dynamic-insert="true">
        <id name="rowID">
            <column name="rowID" sql-type="int" />
            <generator class="increment"/>
        </id>
        <property name="code">
            <column name="code" sql-type="varchar(8)"/>
        </property>
        <property name="groupID">
            <column name="groupID" sql-type="int"/>
        </property>
        <property name="ddl">
            <column name="ddl" sql-type="datetime"/>
        </property>


    </class>
</hibernate-mapping>