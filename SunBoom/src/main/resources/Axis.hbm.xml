<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="pojo.Axis" table="axis" schema="rsp" dynamic-insert="true">
        <id name="axisID">
            <column name="axisID" sql-type="int" />
            <generator class="increment"/>
        </id>
        <property name="publicityID">
            <column name="publicityID" sql-type="int"/>
        </property>
        <property name="activityID">
            <column name="activityID" sql-type="int"/>
        </property>
        <property name="serial">
            <column name="serial" sql-type="int"/>
        </property>
        <property name="time">
            <column name="time" sql-type="date"/>
        </property>
        <property name="type">
            <column name="type" sql-type="int(1)"/>
        </property>


    </class>
</hibernate-mapping>