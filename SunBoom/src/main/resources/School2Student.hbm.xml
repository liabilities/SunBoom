<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="pojo.School2Student" table="school2student" schema="rsp" dynamic-insert="true">
        <id name="rowID">
            <column name="rowID" sql-type="int" />
            <generator class="increment"/>
        </id>
        <property name="schoolID">
            <column name="schoolID" sql-type="int"/>
        </property>
        <property name="personID">
            <column name="personID" sql-type="int"/>
        </property>
        <property name="enrollmentYear">
            <column name="enrollmentYear" sql-type="int"/>
        </property>
        <property name="identificationTime">
            <column name="identificationTime" sql-type="datetime"/>
        </property>

    </class>
</hibernate-mapping>