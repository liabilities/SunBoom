<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="pojo.TempGroup" table="tempgroup" schema="rsp" dynamic-insert="true">
        <id name="groupID">
            <column name="groupID" sql-type="int" />
            <generator class="increment"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(45)"/>
        </property>
        <property name="tag">
            <column name="tag" sql-type="varchar(45)" />
        </property>
        <property name="scale">
            <column name="scale" sql-type="int"/>
        </property>
        <property name="presentMemberNum">
            <column name="presentMemberNum" sql-type="int"/>
        </property>
        <property name="signature">
            <column name="signature" sql-type="varchar(45)"/>
        </property>
        <property name="memberRequestion">
            <column name="memberRequestion" sql-type="varchar(255)"/>
        </property>
        <property name="goal">
            <column name="goal" sql-type="varchar(255)" />
        </property>
        <property name="avatar">
            <column name="avatar" sql-type="varchar(255)"/>
        </property>
        <property name="registerTime">
            <column name="registerTime" sql-type="datetime" />
        </property>
        <property name="verify">
            <column name="verify" sql-type="int(1)"/>
        </property>
        <property name="fellowGroup">
            <column name="fellowGroup" sql-type="int"/>
        </property>
        <property name="fellowPerson">
            <column name="fellowPerson" sql-type="int" />
        </property>

    </class>
</hibernate-mapping>