<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="pojo.TempGroup" table="tempgroup" schema="rsp" dynamic-insert="true">
        <id name="tempGroupID">
            <column name="tempGroupID" sql-type="int(11)" />
            <generator class="increment"/>
        </id>
        <property name="universityID">
            <column name="universityID" sql-type="int(11)"/>
        </property>
        <property name="tag">
            <column name="tag" sql-type="varchar(45)" not-null="false"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(45)"/>
        </property>
        <property name="signature">
            <column name="signature" sql-type="varchar(45)" not-null="false"/>
        </property>
        <property name="introduction">
            <column name="introduction" sql-type="text"  not-null="false"/>
        </property>
        <property name="avatar">
            <column name="avatar" sql-type="text"  not-null="false"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(45)" />
        </property>
        <property name="memberNum">
            <column name="memberNum" sql-type="int"/>
        </property>
        <property name="userName">
            <column name="userName" sql-type="varchar(45)" />
        </property>
        <property name="attestation">
            <column name="attestation" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="request">
            <column name="request" sql-type="text"/>
        </property>
        <property name="goal">
            <column name="goal" sql-type="text" />
        </property>
        <property name="maxNumber">
            <column name="maxNumber" sql-type="int(11)"/>
        </property>

    </class>
</hibernate-mapping>